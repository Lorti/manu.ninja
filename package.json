{
  "name": "manu.ninja",
  "description": "manu.ninja is the personal blog of Manuel Wieser, where he talks about front-end development, web development, coding, games and digital art",
  "version": "1.0.0",
  "author": "Manuel Wieser <office@manuelwieser.com>",
  "dependencies": {
    "gatsby": "^1.6.5",
    "gatsby-link": "^1.6.5",
    "gatsby-plugin-feed": "^1.3.7",
    "gatsby-plugin-google-analytics": "^1.0.1",
    "gatsby-plugin-manifest": "^1.0.1",
    "gatsby-plugin-offline": "^1.0.1",
    "gatsby-plugin-react-helmet": "^1.0.1",
    "gatsby-plugin-sass": "^1.0.8",
    "gatsby-plugin-sharp": "^1.6.3",
    "gatsby-remark-autolink-headers": "^1.4.5",
    "gatsby-remark-copy-linked-files": "^1.5.4",
    "gatsby-remark-images": "^1.5.7",
    "gatsby-remark-prismjs": "^1.2.4",
    "gatsby-remark-responsive-iframe": "^1.4.5",
    "gatsby-remark-smartypants": "^1.4.5",
    "gatsby-source-filesystem": "^1.4.7",
    "gatsby-transformer-remark": "^1.7.4",
    "gatsby-transformer-sharp": "^1.6.2",
    "webfontloader": "^1.6.28"
