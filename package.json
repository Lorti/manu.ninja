{
  "name": "manu.ninja",
  "description": "manu.ninja is the personal blog of Manuel Wieser, where he talks about front-end development, web development, coding, games and digital art",
  "version": "4.0.0",
  "author": "Manuel Wieser <office@manuelwieser.com>",
  "license": "MIT",
  "scripts": {
    "build:clean": "rm -rf public",
    "build:eleventy": "ELEVENTY_ENV=production eleventy",
    "build:service-worker": "workbox generateSW workbox-config.js",
    "build": "npm-run-all build:*",
    "watch": "eleventy --watch",
    "serve": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "format": "prettier --single-quote --write '*.js' 'src/utils/*.js' 'src/**/*.js'"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0-beta.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "glob": "^7.1.6",
    "html-minifier": "^4.0.0",
    "luxon": "^1.23.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-table-of-contents": "^0.4.4",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "purgecss": "^2.1.2",
    "striptags": "^3.1.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "wordcount": "^1.1.1"
  },
  "dependencies": {}
}
